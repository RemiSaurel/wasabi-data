e song."', 'releaseDate': [], 'runtime': [], 'award': [], 'subject': ['Woody Guthrie songs', 'Songs written by Woody Guthrie', '1961 songs'], 'isClassic': False, 'urlITunes': 'https://itunes.apple.com/us/album/id187906715?i=187907087', 'urlAmazon': 'http://www.amazon.com/exec/obidos/redirect?link_code=ur2&tag=wikia-20&camp=1789&creative=9325&path=http%3A%2F%2Fwww.amazon.com%2Fgp%2Fproduct%2FB001389XAG%2Fsr%3D8-1%2Fqid%3D1147400297%2Fref%3Dpd_bbs_1%3F%255Fencoding%3DUTF8', 'urlSpotify': 'https://play.spotify.com/track/4Ll6Id5Raq7HqJGYzgvADx', 'urlAllmusic': 'http://www.allmusic.com/song/mt0012445749', 'urlMusicBrainz': 'http://musicbrainz.org/recording/be7128cb-28ca-41c2-8afd-758c56f90493', 'title': 'Jesus Christ', 'deezer_mapping': [[1064010, 'search-noalbum'], [1064010, 'sptfviaenki']], 'id_song_deezer': '1064010', 'isrc': 'USSM10027996', 'length': '191', 'explicitLyrics': False, 'rank': 286521, 'bpm': '132', 'gain': '-13.1', 'preview': 'http://e-cdn-preview-f.deezer.com/stream/f3f190298064e46624218269d42b64d4-4.mp3', 'availableCountries': ['AD', 'AE', 'AF', 'AG', 'AI', 'AL', 'AM', 'AO', 'AR', 'AT', 'AU', 'AW', 'AZ', 'BA', 'BB', 'BE', 'BF', 'BG', 'BH', 'BI', 'BJ', 'BM', 'BO', 'BR', 'BS', 'BW', 'BY', 'BZ', 'CA', 'CD', 'CF', 'CG', 'CH', 'CI', 'CL', 'CM', 'CN', 'CO', 'CR', 'CU', 'CV', 'CY', 'CZ', 'DJ', 'DK', 'DM', 'DO', 'DZ', 'EC', 'EE', 'EG', 'EH', 'ER', 'ES', 'ET', 'FI', 'FJ', 'FR', 'GA', 'GB', 'GD', 'GE', 'GF', 'GH', 'GI', 'GM', 'GN', 'GP', 'GQ', 'GR', 'GT', 'GW', 'GY', 'HK', 'HN', 'HR', 'HT', 'HU', 'IE', 'IL', 'IQ', 'IR', 'IS', 'IT', 'JM', 'JO', 'KE', 'KG', 'KI', 'KM', 'KN', 'KW', 'KY', 'KZ', 'LB', 'LC', 'LI', 'LR', 'LS', 'LT', 'LU', 'LV', 'LY', 'MA', 'MC', 'MD', 'ME', 'MF', 'MG', 'MH', 'MK', 'ML', 'MN', 'MQ', 'MR', 'MS', 'MT', 'MU', 'MW', 'MX', 'MZ', 'NA', 'NC', 'NE', 'NG', 'NI', 'NL', 'NO', 'NR', 'NZ', 'OM', 'PA', 'PE', 'PF', 'PG', 'PL', 'PN', 'PR', 'PS', 'PT', 'PW', 'PY', 'QA', 'RO', 'RS', 'RU', 'RW', 'SA', 'SB', 'SC', 'SD', 'SE', 'SI', 'SK', 'SL', 'SN', 'SO', 